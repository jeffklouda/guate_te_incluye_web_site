{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      ".git",
      ".gitignore",
      ".npmignore"
    ],
    "rewrites": [{
        "source": "/index*", "function": "getJobs"
    } ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
